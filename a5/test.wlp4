int wain(int* a, int n) {
	int t = 9;
	int l = 0;
	int r = 0;
	int m = 0;
	int temp = 0;
	int temp1 = 0;
	
	
	r = n - 1;

	while(l <= r) {
		temp = l + r;
		m = temp / 2;
	
		temp1 = *(a + m);
		if(temp1 < t) {
			l = m + 1;
		} else {
			if(temp1 > t) {
				r = m - 1;
			} else {
				l = 1;
				r = 0;
			}
		}
	}
	if(temp1 != t) {
		temp1 = 0 - 1;
	} else{}
	return temp1;
}
